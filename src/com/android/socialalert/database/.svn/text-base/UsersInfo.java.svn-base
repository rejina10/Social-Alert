package com.android.socialalert.database;

import java.util.ArrayList;

public class UsersInfo {
    // private String UserId;
    // private String ActiveYesNo;
    private String FirstName;
    private String LastName;
    private String Password;
    private String EmailAddress;
    private String Address;
    private String PhoneNumber;
    private int selectedLangauge;

    // private String UserGuid;
    // private String UserCreatedDateTime;
    // private String UserModifiedDateTime;
    // private String UserDeletedDateTime;

    public int getSelectedLangauge() {
        return selectedLangauge;
    }

    public void setSelectedLangauge(int selectedLangauge) {
        this.selectedLangauge = selectedLangauge;
    }

    private ArrayList<String> AlertType;

    /**
     * @return the FirstName
     */
    public String getFirstName() {
        return FirstName;
    }

    /**
     * @param firstName
     *            the firstName to set
     */
    public void setFirstName(String firstName) {
        this.FirstName = firstName;
    }

    /**
     * @return the LastName
     */
    public String getLastName() {
        return LastName;
    }

    /**
     * @param lastName
     *            the lastName to set
     */
    public void setLastName(String lastName) {
        this.LastName = lastName;
    }

    /**
     * @return the LastName
     */
    public String getPassword() {
        return Password;
    }

    /**
     * @param lastName
     *            the lastName to set
     */
    public void setPassword(String password) {
        this.Password = password;
    }

    /**
     * @return the EmailAddress
     */
    public String getEmailAddress() {
        return EmailAddress.trim();
    }

    /**
     * @param emailAddress
     *            the emailAddress to set
     */
    public void setEmailAddress(String emailAddress) {
        this.EmailAddress = emailAddress.trim();
    }

    /**
     * @return the Address
     */
    public String getAddress() {
        return Address;
    }

    /**
     * @param address
     *            the address to set
     */
    public void setAddress(String address) {
        this.Address = address;
    }

    /**
